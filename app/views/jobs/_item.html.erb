  <!-- <table class="table table-boldered"> -->
    <thead>
      <tr>
        <td>需求</td>
        <td>种类</td>
        <td>薪资下限</td>
        <td>薪资上限</td>
        <td>需求时间</td>
        <td>刊登时间</td>
      </tr>
    </thead>
    <tbody>
    <% @jobs.each do |job| %>
    <tr>
      <td>
        <%= link_to(job.title, job_path(job)) %>
      </td>
      <!-- <td>
       <i class="fa fa-fire"></i>
     </td> -->
      <td>
       <%= link_to(job.field, job_path(job)) %>
      </td>
      <td><%= job.wage_lower_bound %>
      </td>
      <td><%= job.wage_upper_bound %>
      </td>
      <td>
        <%= job.require_time %>
      </td>
      <td>
        <%= job.created_at.to_s(:short) %>
      </td>
    </tr>
    <% end %>

  </tbody>
  </table>
